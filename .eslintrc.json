{
    "extends": "airbnb",
    "plugins": [
      "react"
    ],
    "globals": {
      "googletag": true,
      "describe": true,
      "expect": true,
      "it": true,
      "beforeEach": true,
      "afterEach": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "react/prefer-es6-class": 0
    }
}